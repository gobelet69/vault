{
  "name": "personnalsite",
  "main": "pdfs.js",
  "compatibility_date": "2024-09-23",
  "r2_buckets": [
    { "binding": "BUCKET", "bucket_name": "my-pdfs" }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "pdf-db",
      "database_id": "REPLACE_WITH_YOUR_D1_ID_FROM_STEP_1"
    }
  ]
}